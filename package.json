    "workbox-strategies": "^7.3.0",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "scripts": {
    "start": "env-cmd -f .env.development react-scripts start",
    "build": "react-scripts build && cp public/_redirects build/",
